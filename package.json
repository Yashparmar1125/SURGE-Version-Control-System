{
  "name": "surge-vcs",
  "version": "1.0.0",
  "description": "A modern, user-friendly version control system",
  "main": "src/index.js",
  "bin": {
    "surge": "./src/index.js"
  },
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "test": "jest",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write src/**/*.js",
    "build": "babel src -d dist",
    "coverage": "jest --coverage",
    "security-check": "npm audit",
    "prepare": "husky install"
  },
  "keywords": [
    "vcs",
    "version-control",
    "git-alternative"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "better-sqlite3": "^8.6.0",
    "chalk": "^5.4.1",
    "commander": "^13.1.0",
    "crypto-js": "^4.2.0",
    "diff": "^7.0.0",
    "graceful-fs": "^4.2.11",
    "lockfile": "^1.0.4",
    "node-fetch": "^3.3.2",
    "surge-vcs": "file:",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^27.9.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "prettier": "^3.2.5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/surge-vcs.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/surge-vcs/issues"
